//gratest of three no.//
#include<iostream>
using namespace std;
int main(){
    int a,b,c;
cin>>a>>b>>c;
if(a>b){
    if(a>c){
    cout<<"greatest no is:"<<a<<endl;
    }
    else{
        cout<<"greatest no is:"<<c<<endl;
    }
}
else{
    if(b>c){
        cout<<"greatest no is:"<<b<<endl;
    }
    else{
        cout<<"greatest no is:"<<c<<endl;
    }
}
return 0;
}
